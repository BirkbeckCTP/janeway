<?xml version="1.0" encoding="UTF-8"?>
<doi_batch xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.crossref.org/schema/4.4.2 http://www.crossref.org/schema/crossref4.4.2.xsd"
           xmlns="http://www.crossref.org/schema/4.4.2" version="4.4.2">
    <head>
        <doi_batch_id>{{ batch_id }}</doi_batch_id>
        <timestamp>{{ now|date:"YmdHis" }}{{ timestamp_suffix }}</timestamp>
        <depositor>
            <depositor_name>{{ depositor_name }}</depositor_name>
            <email_address>{{ depositor_email }}</email_address>
        </depositor>
        <registrant>{{ registrant }}</registrant>
    </head>
    <body>
    {% for review in reviews %}
        {% include "common/identifiers/crossref_review.xml" %}
    {% endfor %}
    </body>
</doi_batch>
